package org.easy;

import java.util.function.IntPredicate;

public class Pred {

	public static void main(String[] args) {
		IntPredicate lessThan18 = new IntPredicate() {
			
			@Override
			public boolean test(int value) {
				if (value > 18) {
					return true;
				} else {
					return false;
				}
				
			}
		};

	}

}
