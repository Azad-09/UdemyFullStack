package org.easy;

//functional interface consist of more than 1 method (Lambda expression is not applicable for this Interface)
interface Lambda{
	public void demo();
//	public void radhe();
}

public class Lamb {
	

	public static void main(String[] args) {
		Lambda lambda = ()->{
			System.out.println("Bolo Haleluyah!!");
			System.out.println("Radhe Shyam");
		};
		
		lambda.demo();
	}
}
