package org.easy;

interface Lambda{
	public void demo();
	public void radhe();
}

public class Lamb {
	

	public static void main(String[] args) {
		Lambda lambda = new Lambda() {
			
			@Override
			public void radhe() {
				System.out.println("Bolo Radhe Radhe");
			}
			
			@Override
			public void demo() {
				System.out.println("Demo tha yaar");
				
			}
		};
	}

}
